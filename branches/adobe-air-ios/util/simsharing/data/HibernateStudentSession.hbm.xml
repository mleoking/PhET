<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.colorado.phet.simsharing.server.hibernate">
    <class name="HibernateStudentSession" table="HibernateStudentSession">
        <id name="index" type="long" column="id"/>
        <property name="name"/>
        <property name="sim"/>
        <property name="time" type="timestamp"/>
    </class>
</hibernate-mapping>
