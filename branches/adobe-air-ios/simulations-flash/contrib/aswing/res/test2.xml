<?xml version="1.0" encoding="UTF-8"?>
<aw:aswing xmlns:aw="http://awml.aswing.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://awml.aswing.org ../awml.xsd ">
    <frame id="frame" namespace="main" owner="_root" modal="true" enabled="true" width="500" height="400" closable="true" title="Test Title" resizable="true">
        <soft-box axis="y" opaque="true">
                <background alpha="100" rgb="0x00FF00"></background>
            
            <text-field id="group" text="AAAA">
                        <background alpha="100" rgb="F3F3F3"></background>
                <foreground alpha="100" rgb="000000"></foreground>
                <font embeded="false" underline="false" italic="false" bold="false" size="11" name="Tahoma"></font>
            </text-field>
        
            <button id="getTextBtn" text="getText" on-press="frame.onGetTextButtonClick"></button>
        </soft-box>
    </frame>
</aw:aswing>

