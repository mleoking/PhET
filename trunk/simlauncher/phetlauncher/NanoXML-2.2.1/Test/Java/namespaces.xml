<!DOCTYPE Foo [
        <!ELEMENT Foo (ns:Bar)>
        <!ATTLIST Foo
                xmlns CDATA #FIXED 'http://nanoxml.n3.net/foo'
                a     CDATA #REQUIRED>

        <!ELEMENT ns:Bar (Blah)>
        <!ATTLIST ns:Bar
                xmlns:ns CDATA #FIXED 'http://nanoxml.n3.net/bar'>

        <!ELEMENT Blah EMPTY>
        <!ATTLIST Blah
                x    CDATA #REQUIRED
                ns:x CDATA #REQUIRED>
        ]>

<Foo a='test'>
    <ns:Bar>
        <Blah x='1' ns:x='2'/>
    </ns:Bar>
</Foo>

