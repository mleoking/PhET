<define name="action">
  <choice>
 
  <!--   queryWMI - Query WMI and return results
         Specific properties for <queryWMI>:

         <class>: Class
         <fields>: List of fields to return
         <namespace>: Namespace
         <variable>: 
         <where>: Query to pass to WMI
  -->
  <element name="queryWMI">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="class"/>
        <element name="class"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="fields"/>
        <element name="fields"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="namespace"/>
        <element name="namespace"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="variable"/>
        <element name="variable"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="where"/>
        <element name="where"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   changeExecutableResources - Change resource information of a Windows executable
         Specific properties for <changeExecutableResources>:

         <path>: Path to the Windows executable to modify
         <windowsResourceComments>: Comments for resources embedded in Windows executable
         <windowsResourceCompanyName>: Company Name for resources embedded in Windows executable
         <windowsResourceFileDescription>: File Description for resources embedded in Windows executable
         <windowsResourceFileVersion>: File version for resources embedded in Windows executable
         <windowsResourceInternalName>: Internal Name for resources embedded in Windows executable
         <windowsResourceLegalCopyright>: Legal Copyright for resources embedded in Windows executable
         <windowsResourceLegalTrademarks>: Legal Trademarks for resources embedded in Windows executable
         <windowsResourceOriginalFilename>: Original Filename for resources embedded in Windows executable
         <windowsResourceProductName>: Product Name for resources embedded in Windows executable
         <windowsResourceProductVersion>: Product Version for resources embedded in Windows executable
  -->
  <element name="changeExecutableResources">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="path"/>
        <element name="path"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceComments"/>
        <element name="windowsResourceComments"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceCompanyName"/>
        <element name="windowsResourceCompanyName"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceFileDescription"/>
        <element name="windowsResourceFileDescription"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceFileVersion"/>
        <element name="windowsResourceFileVersion"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceInternalName"/>
        <element name="windowsResourceInternalName"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceLegalCopyright"/>
        <element name="windowsResourceLegalCopyright"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceLegalTrademarks"/>
        <element name="windowsResourceLegalTrademarks"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceOriginalFilename"/>
        <element name="windowsResourceOriginalFilename"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceProductName"/>
        <element name="windowsResourceProductName"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="windowsResourceProductVersion"/>
        <element name="windowsResourceProductVersion"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   pwd - Get the current working directory
         Specific properties for <pwd>:

         <variable>: Variable to which to save the current working directory.
  -->
  <element name="pwd">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="variable"/>
        <element name="variable"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   unix2dos - Convert plain text files in Unix format to DOS format.
         Specific properties for <unix2dos>:

         <excludeFiles>: Patterns to exclude files
         <files>: File patterns to apply action to
         <matchHiddenFiles>: Whether or not to attempt to match Windows hidden files
  -->
  <element name="unix2dos">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="excludeFiles"/>
        <element name="excludeFiles"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="files"/>
        <element name="files"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="matchHiddenFiles"/>
        <element name="matchHiddenFiles"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   deleteOSXService - Delete Mac OS X service.
         Specific properties for <deleteOSXService>:

         <scope>: Scope of service  - Allowed values:
                  user: Per User
                  system: System Wide
         <serviceName>: Identifier for the service name
  -->
  <element name="deleteOSXService">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="scope"/>
        <element name="scope"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="serviceName"/>
        <element name="serviceName"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   encodeBase64 - Encode a string using base64.
         Specific properties for <encodeBase64>:

         <text>: Text to process
         <variable>: Variable to save the final result to.
  -->
  <element name="encodeBase64">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="text"/>
        <element name="text"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="variable"/>
        <element name="variable"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   getFileInfo - Gets Information About File Or Directory
         Specific properties for <getFileInfo>:

         <followSymLinks>: Whether or not to follow or not symbolic links
         <path>: Path
         <type>: Type of information to retrieve  - Allowed values:
                 size: Size
                 type: Type
         <variable>: Variable to store result in
  -->
  <element name="getFileInfo">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="followSymLinks"/>
        <element name="followSymLinks"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="path"/>
        <element name="path"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="type"/>
        <element name="type"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="variable"/>
        <element name="variable"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   setWindowsACL - Change Windows ACL permissions of a file or directory
         Specific properties for <setWindowsACL>:

         <excludeFiles>: Patterns to exclude files
         <files>: File patterns to apply action to
         <matchHiddenFiles>: Whether or not to attempt to match Windows hidden files
         <permissions>: Permissions for matching files or directories
         <recurseContainers>: Whether to apply recursively or not to container descendant
         <recurseObjects>: Whether to apply recursively or not to object descendant
         <recurseOneLevelOnly>: Just one level recursion if applicable
         <self>: Whether to apply or not to the specified file
         <users>: Comma separated list of users to modify access permissions for
  -->
  <element name="setWindowsACL">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="action"/>
        <element name="action"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="excludeFiles"/>
        <element name="excludeFiles"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="files"/>
        <element name="files"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="matchHiddenFiles"/>
        <element name="matchHiddenFiles"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="permissions"/>
        <element name="permissions"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="recurseContainers"/>
        <element name="recurseContainers"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="recurseObjects"/>
        <element name="recurseObjects"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="recurseOneLevelOnly"/>
        <element name="recurseOneLevelOnly"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="self"/>
        <element name="self"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="users"/>
        <element name="users"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   setEncryptionPassword - Specifies and verifies password to use for copying files from installer
         Specific properties for <setEncryptionPassword>:

         <password>: Password to use; action will throw error if password specified does not match password from build time
  -->
  <element name="setEncryptionPassword">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="password"/>
        <element name="password"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   copyFile - Create a copy of a file. The destination file will be overwritten if it already exists
         Specific properties for <copyFile>:

         <destination>: Path to where the file will be copied.
         <excludeFiles>: Patterns to exclude files
         <matchHiddenFiles>: Whether or not to attempt to match Windows hidden files
         <origin>: Path to the original file.
  -->
  <element name="copyFile">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="destination"/>
        <element name="destination"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="excludeFiles"/>
        <element name="excludeFiles"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="matchHiddenFiles"/>
        <element name="matchHiddenFiles"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="origin"/>
        <element name="origin"><text/></element>
      </choice>
    </optional>
    </interleave>
  </element>
 
 
  <!--   showPasswordQuestion - Ask the user to enter a password.
         Specific properties for <showPasswordQuestion>:

         <text>: Question message that will be shown.
         <title>: Dialog window title.
         <variable>: Variable name where the password will be stored.
  -->
  <element name="showPasswordQuestion">
    <interleave>
    <ref name="actionCommonProperties"/>
    <optional>
      <choice>
        <attribute name="text"/>
        <element name="text"><text/></element>
      </choice>
    </optional>
    <optional>
