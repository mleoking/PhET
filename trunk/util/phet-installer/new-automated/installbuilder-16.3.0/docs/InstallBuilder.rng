          <ref name="action"/>
        </zeroOrMore>
      </element>
    </optional>
    <optional>
      <element name="readmeFileList">
        <zeroOrMore>
          <ref name="readmeFile"/>
        </zeroOrMore>
      </element>
    </optional>
    <optional>
