<!-- These properties are common to all actions -->
<define name="actionCommonProperties">
  <interleave>
    <optional>
      <choice>
        <attribute name="abortOnError"/>
        <element name="abortOnError"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="breakpoint"/>
        <element name="breakpoint"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="customErrorMessage"/>
        <element name="customErrorMessage"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="explanation"/>
        <element name="explanation"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="progressText"/>
        <element name="progressText"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="ruleEvaluationLogic"/>
        <element name="ruleEvaluationLogic"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="run"/>
        <element name="run"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="show"/>
        <element name="show"><text/></element>
      </choice>
    </optional>
    <optional>
      <choice>
        <attribute name="showMessageOnError"/>
        <element name="showMessageOnError"><text/></element>
      </choice>
    </optional>
    <optional>
      <element name="onErrorActionList">
        <zeroOrMore>
          <ref name="action"/>
        </zeroOrMore>
      </element>
    </optional>
    <optional>
      <element name="ruleList">
        <zeroOrMore>
          <ref name="rule"/>
        </zeroOrMore>
      </element>
    </optional>
  </interleave>
</define>

 
