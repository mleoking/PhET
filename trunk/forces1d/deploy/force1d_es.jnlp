<jnlp
    spec="1.0+"
    codebase="http://www.colorado.edu/physics/phet/simulations/"
    href="force1d/force1d_es.jnlp">
    <information>
        <title>Forces in 1D</title>
        <vendor>PhET(tm)</vendor>
        <homepage href="http://www.colorado.edu/physics/phet/"/>
        <description>Forces in 1D</description>
        <description kind="short">Forces1D</description>
        <icon href="../Design/Assets/images/Phet-Kavli-logo.jpg"/>
        <icon kind="splash" href="../Design/Assets/images/Phet-Kavli-logo.jpg"/>
        <offline-allowed/>
    </information>
    <security>
    </security>

    <resources>
        <j2se version="1.4+"/>
        <jar href="force1d/force1d_es.jar"/>
        <jar href="force1d/Force1DStrings_es.jar"/>
        <jar href="common/CommonStrings_es.jar"/>
        <jar href="force1d/lib/charts.jar"/>
        <jar href="force1d/lib/phetcommon.jar"/>
    </resources>
    <application-desc main-class="edu.colorado.phet.forces1d.Force1DModule">
        <argument>user.language=es</argument>	
    </application-desc>
</jnlp>