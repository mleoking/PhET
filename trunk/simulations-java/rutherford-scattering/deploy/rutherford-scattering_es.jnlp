<!-- Spanish (es) JNLP File for PhET "Rutherford Scattering" simulation -->
<jnlp
  spec="1.0+"
  codebase="http://phet-web.colorado.edu/simulations"
  href="rutherford-scattering/rutherford-scattering_es.jnlp">
  <information>
    <title>Dispersion De Rutherford</title>
    <vendor>Universidad de Colorado, Departamento de la Fisica</vendor>
    <homepage href="http://phet-web.colorado.edu"/>
    <icon href="icons/PhET.gif"/>
    <icon kind="splash" href="icons/PhET.gif"/>
    <offline-allowed/>
  </information>
  <resources>
    <j2se version="1.4+"/>
    <jar href="rutherford-scattering/rutherford-scattering.jar"/>
  </resources>
  <application-desc main-class="edu.colorado.phet.rutherfordscattering.RSApplication">
        <argument>user.language=es</argument>
  </application-desc>
</jnlp>