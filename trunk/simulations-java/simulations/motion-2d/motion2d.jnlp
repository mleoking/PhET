<jnlp
        spec="1.0+"
        codebase="http://www.colorado.edu/physics/phet/dev/"
        href="motion2d/@VERSION@/motion2d.jnlp">
    <information>
        <title>Motion in 2D</title>
        <vendor>PhET</vendor>
        <homepage href="http://www.colorado.edu/physics/phet/"/>
        <description>Depicts velocity and acceleration of a particle.</description>
        <description kind="short">A velocity and acceleration simulation.</description>
        <icon href="../Design/Assets/images/Phet-Kavli-logo.jpg"/>
        <icon kind="splash" href="../Design/Assets/images/Phet-Kavli-logo.jpg"/>
        <offline-allowed/>
    </information>
    <resources>
        <j2se version="1.4+"/>
        <jar href="motion2d/@VERSION@/motion2d-pro.jar"/>
    </resources>
    <application-desc main-class="@MAIN-CLASS@"/>
</jnlp>