  </information> 
  <resources> 
    <j2se version="1.4+"/> 
    <jar href="optical-tweezers/opticaltweezers.jar"/> 
  </resources> 
  <application-desc main-class="edu.colorado.phet.opticaltweezers.OTApplication">
        <argument>user.language=es</argument>
  </application-desc>
</jnlp> 
