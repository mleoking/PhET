<!-- Spanish (es) JNLP File for PhET "Optical Tweezers" simulation --> 
<jnlp
  spec="1.0+" 
  codebase="http://phet-web.colorado.edu/simulations"
  href="optical-tweezers/optical-tweezers_es.jnlp"> 
  <information> 
    <title>Optical Tweezers</title>
    <vendor>Universidad de Colorado, Departamento de la Fisica</vendor> 
    <homepage href="http://phet-web.colorado.edu"/> 
