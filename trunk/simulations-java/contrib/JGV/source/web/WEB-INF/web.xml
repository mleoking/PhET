<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>3D Viewer Sample Application</display-name>
  <description>3D Viewer Sample Application</description>
  <servlet>
    <servlet-name>AcceptMessageServlet</servlet-name>
    <display-name>Accept a message</display-name>
    <servlet-class>org.dm.servlet.AcceptMessageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DisplayObjectServlet</servlet-name>
    <display-name>Display an object</display-name>
    <servlet-class>org.dm.servlet.DisplayObjectServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GenerateObjectServlet</servlet-name>
    <display-name>Generate an object on the server</display-name>
    <servlet-class>org.dm.servlet.GenerateObjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayObjectServlet</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GenerateObjectServlet</servlet-name>
    <url-pattern>/generate.xml</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AcceptMessageServlet</servlet-name>
    <url-pattern>/server</url-pattern>
  </servlet-mapping>
</web-app>
