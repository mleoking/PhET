<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
    <rule>
        <from>^/simulation/([^/]*)/([^/]*)$</from>
        <to>/simulationpage.jsp?projectName=$1&amp;simulationName=$2</to>
    </rule>
</urlrewrite>