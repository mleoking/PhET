<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="edu.colorado.phet.wickettest.test">
    <class name="BasicSimulation" table="basic_simulation">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="name"/>
        <property name="type"/>
        <many-to-one name="project" class="BasicProject"/>
        <set name="localizedSimulations" inverse="true">
            <key column="simulation"/>
            <one-to-many class="BasicLocalizedSimulation"/>
        </set>
    </class>
</hibernate-mapping>