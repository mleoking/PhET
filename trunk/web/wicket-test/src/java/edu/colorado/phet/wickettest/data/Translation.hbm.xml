<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="edu.colorado.phet.wickettest.data.Translation" table="translation">
        <cache usage="read-write"/>
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="locale" type="locale"/>
        <set name="translatedStrings" inverse="true">
            <key column="translation"/>
            <one-to-many class="edu.colorado.phet.wickettest.data.TranslatedString"/>
        </set>
    </class>
</hibernate-mapping>