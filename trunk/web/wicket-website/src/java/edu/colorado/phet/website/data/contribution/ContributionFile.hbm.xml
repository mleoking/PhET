<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="edu.colorado.phet.website.data.contribution.ContributionFile" table="contribution_file">
        <cache usage="read-write"/>
        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <many-to-one name="contribution" class="edu.colorado.phet.website.data.contribution.Contribution" not-null="true"
                     lazy="false"/>
        <property name="filename"/>
        <property name="location"/>
        <property name="size"/>

    </class>
</hibernate-mapping>