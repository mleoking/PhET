<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="edu.colorado.phet.wickettest.data.TranslatedString" table="translated_string">
        <cache usage="read-write"/>
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="key"/>
        <property name="value" length="4048"/>
        <property name="createdAt" type="timestamp"/>
        <property name="updatedAt" type="timestamp"/>
        <many-to-one name="translation" class="edu.colorado.phet.wickettest.data.Translation" column="translation"
                     not-null="true"/>
    </class>
</hibernate-mapping>